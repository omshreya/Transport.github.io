	<!-- Search -->

	<div class="home_search">
		<div class="container">
			<div class="row">
				<div class="col">
					<div class="home_search_container">
						<div class="home_search_title">Make your trip</div>
						<div class="home_search_content">
							<form action="fixRoute" class="home_search_form" id="home_search_form" method="get">
								<div class="d-flex flex-lg-row flex-column align-items-start justify-content-lg-between justify-content-start">
									<select class="mdb-select md-form" name="pickup">
										  <option value="" disabled selected>Choose your pickup</option>
										  <option value="Karad">Karad</option>
										  <option value="Islampur">Islampur</option>
										  <option value="Mumbai">Mumbai</option>
										  <option value="Kolhapur">Kolhapur</option>
										  <option value="Satara">Satara</option>
									</select>
									<select class="mdb-select md-form" name="drop">
										  <option value="" disabled selected>Choose your Drop</option>
										  <option value="Nashik" name="Nashik">Nashik</option>
										  <option value="Mumbai" name="Mumbai">Mumbai</option>
										  <option value="Aurangbad" name="Aurangbad">Aurangbad</option>
										  <option value="Pune" name="Pune">Pune</option>
										  <option value="Alibaug" name="Alibaug">Alibaug</option>
									</select>
									<input type="time" class="search_input search_input_3" name="arrivalTime" placeholder="Arrival Time" required="required">
									<input type="time" class="search_input search_input_4" name="departureTime" placeholder="Departure Time" required="required">
									<input type="text" name="tripDate" placeholder="YYYY-MM-DD" required 
											pattern="(?:19|20)[0-9]{2}-(?:(?:0[1-9]|1[0-2])-(?:0[1-9]|1[0-9]|2[0-9])|(?:(?!02)(?:0[1-9]|1[0-2])-(?:30))|(?:(?:0[13578]|1[02])-31))" 
											title="Enter a date in this format YYYY-MM-DD"/>
									Bus:<input type="radio" name="transport" value="Bus">
									Travel:<input type="radio" name="transport" value="Travel">	<br><br>	
									<input type="submit" value="Check Availability">
								</div>
							</form>
